{
   "name":"Translate C# to a RemObjects language",
   "description":"Translates C# examples to Mercury VB, Oxygene Pascal or RemObjects Swift",
   "version":"1.1",
   "manifest_version":2,
   "permissions": ["<all_urls>","webRequest","webRequestBlocking"],
   "content_scripts": [
      {
      "matches": ["https://docs.microsoft.com/*", "https://stackoverflow.com/*", "https://www.codeproject.com/*"],
      "js": ["myscript.js"]
      }
   ],
   "background": {
      "scripts": ["background.js"]
   },
   "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
   }

}
